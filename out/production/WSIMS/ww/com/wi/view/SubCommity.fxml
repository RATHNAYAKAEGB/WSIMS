<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXTextField?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="737.0" prefWidth="478.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ww.com.wi.controller.SubCommityController">
   <children>
      <Label alignment="CENTER" prefHeight="75.0" prefWidth="478.0" style="-fx-background-color: #000080;" text="Sub Commity" textFill="WHITE">
         <font>
            <Font name="Agency FB Bold" size="27.0" />
         </font>
      </Label>
      <JFXTextField fx:id="txtSubId" editable="false" labelFloat="true" layoutX="24.0" layoutY="128.0" prefHeight="31.0" prefWidth="404.0" promptText="Sub Commity ID">
         <font>
            <Font name="Bell MT" size="18.0" />
         </font>
      </JFXTextField>
      <JFXTextField fx:id="txtName" labelFloat="true" layoutX="24.0" layoutY="193.0" prefHeight="31.0" prefWidth="404.0" promptText="Name">
         <font>
            <Font name="Bell MT" size="18.0" />
         </font>
      </JFXTextField>
      <JFXTextField fx:id="txtCreatedBy" editable="false" labelFloat="true" layoutX="26.0" layoutY="256.0" prefHeight="31.0" prefWidth="404.0" promptText="Create By">
         <font>
            <Font name="Bell MT" size="18.0" />
         </font>
      </JFXTextField>
      <TableView fx:id="tblSubCommity" layoutX="12.0" layoutY="394.0" prefHeight="308.0" prefWidth="449.0">
        <columns>
          <TableColumn prefWidth="135.0" text="Sub Commity ID" />
          <TableColumn prefWidth="159.0" text="Name" />
            <TableColumn prefWidth="154.0" text="Created By" />
        </columns>
      </TableView>
      <JFXButton fx:id="btnSave" layoutX="165.0" layoutY="332.0" onAction="#btnSave_OnAction" prefHeight="32.0" prefWidth="80.0" style="-fx-background-color: green;" text="Save" textFill="WHITE">
         <font>
            <Font name="Arial Narrow" size="16.0" />
         </font>
      </JFXButton>
      <JFXButton fx:id="btnUpdate" layoutX="261.0" layoutY="332.0" onAction="#btnUpdate_OnAction" prefHeight="32.0" prefWidth="80.0" style="-fx-background-color: #808000;" text="Update" textFill="WHITE">
         <font>
            <Font name="Arial Narrow" size="16.0" />
         </font>
      </JFXButton>
      <JFXButton fx:id="btnDelete" layoutX="352.0" layoutY="332.0" onAction="#btnDeleteOnAction" prefHeight="32.0" prefWidth="80.0" style="-fx-background-color: #008080;" text="Delete" textFill="WHITE">
         <font>
            <Font name="Arial Narrow" size="16.0" />
         </font>
      </JFXButton>
      <Button fx:id="btnBack" layoutX="14.0" layoutY="22.0" mnemonicParsing="false" onAction="#btnBack_OnAction" text="Back">
         <font>
            <Font name="Arial Bold" size="14.0" />
         </font>
      </Button>
